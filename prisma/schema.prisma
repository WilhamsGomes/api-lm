generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Invoice {
  id                 String @id @default(uuid()) @db.Uuid
  num_client         Int
  account_month      Int
  account_year       Int
  url_file           String?
  amount_electricity Int
  value_electricity  Float
  amount_sceee       Int
  value_sceee        Float
  amount_compensated Int
  value_compensated  Float
  value_street       Float

  @@map("invoices")
}

